html
	head
		title facebook callback
	body
		script(src="/s/scripts/lib/post-message-polyfill.js")
		script
			var code = location.search.substring(6);
			var auth = {
				command:"signin",
				auth: {
					facebook:{
						code: code
					}
				}
			};
			window.opener.postMessage(auth,"*");
			window.close();