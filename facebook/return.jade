html
	head
		title facebook callback
	body
		#text
			Hello world 	
		script
			var code = location.search.substring(6);
			var auth = {
				command:"signin",
				auth: {
					facebook:{
						code: code
					}
				}
			};
			if (window && window.opener) {
				window.opener.postMessage(auth,"*");
				window.close();
			} else {
				localStorage.authValue = JSON.stringify(auth);
			}